{
    "name": "ntzm/php-cs-fixer-playground",
    "type": "project",
    "description": "An interactive playground for PHP-CS-Fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Nat Zimmermann",
            "email": "nat@ntzm.io"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/orm": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.11",
        "guzzlehttp/guzzle": "^6.3",
        "league/container": "^2.4",
        "nikic/fast-route": "^1.3",
        "ramsey/uuid": "^3.7",
        "sebastian/diff": "^3.0",
        "symfony/http-foundation": "^4.0",
        "twig/twig": "^2.4",
        "zendframework/zend-code": "^3.3"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2"
        }
    },
    "autoload": {
        "psr-4": {
            "PhpCsFixerPlayground\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpCsFixerPlayground\\Tests\\": "tests/"
        }
    }
}
